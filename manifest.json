{
  "manifest_version": 3,
  "name": "Momentum App",
  "version": "1.0.0",
  "description": "Momentum App with Valorant Quotes",
  "icons": {
    "48": "assets/images/icon.png",
    "128": "assets/images/icon.png"
  },
  "action": {
    "default_title": "mumble-dev Momentum App",
    "default_icon": "assets/images/icon.png"
  },
  "chrome_url_overrides" : {
    "newtab" : "index.html"
  },
  "permissions": ["storage"]
}
